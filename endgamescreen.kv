# All classes related to the end game screen

<Scores@Label>:
    markup: True
    font_size: '30sp'

<EndGameScreen>:
    on_enter: app.music.currentSong = 'New_set_song'
    BoxLayout:
        orientation: "vertical"
        Label:
            id: title
            text: 'Game Over'
            font_size: '40sp'
        Scores:
            id: score1
        Scores:
            id: score2
        Scores:
            id: score3
        Scores:
            id: score4
        Scores:
            id: score5
        Button:
            text: 'Back to Main Screen'
            on_press: root.game.current = 'intro'
